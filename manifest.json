{
	"name": "Tabbed Tabs",
	"description": "Keeps new tabs on tab",
	"version": "0.1",
	"incognito": "split",
	"manifest_version": 2,
	"permissions": [
	"<all_urls>",
	"storage",
	"contextMenus",
	"activeTab"],
	"background": {"scripts": ["background.js"]},
	"browser_action": {
         "title": "Your Browser Action Name"
    },
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"    
	}